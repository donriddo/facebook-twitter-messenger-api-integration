{
  "globals": {
    "_": true,
    "_bcrypt": true,
    "_jwt": true,
    "_uuid": true,
    "_pager": true,
    "_validator": true,
    "EmailService": true,
    "NotificationService": true,
    "ResponseService": true,
    "ValidationService": true,
    "JwtService": true,
    "QueryService": true,
    "User": true,
    "Setup": true,
    "FacebookProfile": true,
    "FacebookService": true,
    "sails": true
  },
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": "off",
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "ignore"
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}